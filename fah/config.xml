<!--
This is a minimal fah configuration file for computing
with a CPU.
-->
<config>
  <!-- Client Control -->
  <fold-anon v='false'/>

  <!-- Remote Command Server -->
  <command-allow-no-pass v='127.0.0.1 your.ip.address'/>
  <command-port v='36330'/>

  <!-- HTTP Server -->
  <allow v='127.0.0.1 your.ip.address'/>
  <web-allow v='127.0.0.1 your.ip.address'/>

  <!-- Folding Slot Configuration -->
  <gpu v='false'/>

  <!-- Slot Control -->
  <power v='medium'/>

  <!-- User Information -->
  <passkey v='yourpasskey'/>
  <team v='teamcode'/>
  <user v='username'/>

  <!-- Folding Slots -->
  <slot id='0' type='CPU'/>
</config>
